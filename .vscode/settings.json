{
    "workbench.iconTheme": "vs-minimal",
    "typescript.inlayHints.enumMemberValues.enabled": false,
    "java.compile.nullAnalysis.mode": "automatic",
    "java.project.referencedLibraries": [
        "bazel-genfiles/**/*.jar",
        "bazel-bin/**/*.jar"
    ],
    "java.jdt.ls.vmargs": "-XX:+UseParallelGC -XX:GCTimeRatio=4 -XX:AdaptiveSizePolicyWeight=90 -Dsun.zip.disableMemoryMapping=true -Xmx2G -Xms100m -Xlog:disable",
    "java.project.sourcePaths": [
        "app",
        "dev"
    ],
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        ".devcontainer": true,
        ".eclipse": true,
        ".git": true,
        ".github": true,
        ".idea": true,
        ".ijwb": true,
        ".pnpm-store": true,
        ".vscode": true,
        "app": true,
        "cfg": true,
        "dev": true,
        "dist": true,
        "node_modules": true,
        "scripts": true,
        "bazel-bin": true,
        "bazel-logos": true,
        "bazel-out": true,
        "bazel-testlogs": true
    },
}